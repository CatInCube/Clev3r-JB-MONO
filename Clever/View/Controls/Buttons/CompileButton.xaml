<UserControl x:Class="Clever.View.Controls.Buttons.CompileButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Clever.View.Controls.Buttons"
             xmlns:compile_but="clr-namespace:Clever.View.Controls.Buttons.CompileButtons"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="1000">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <compile_but:ButtonConnect Margin="5,0,0,0"/>
            <compile_but:ButtonDownload Margin="10,0,0,0"/>
            <compile_but:ButtonCompile Margin="10,0,0,0"/>
            <compile_but:ButtonCompileAndRun Margin="10,0,0,0"/>
            <compile_but:ButtonStop Margin="10,0,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>
